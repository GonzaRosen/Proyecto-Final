    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Proyectito Finalsito"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PerfilUsuario" />
        <activity android:name=".FiltrosBusqueda" />
        <activity android:name=".RegistracionUsuario" />
        <activity android:name=".RecuperarContrasenia" />
        <activity android:name=".ConfirmacionRecu" />
        <activity android:name=".Loginn" />
        <activity android:name=".Settings"></activity>
    </application>

</manifest>